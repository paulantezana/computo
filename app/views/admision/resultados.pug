extends ../layout

block content
    include blocks/menu
    .Container
        .SiteHeader
            h1.SiteHeader-title Resultados de exámenes de admisión
            .SiteHeader-subTitle Resultados anteriores
        div
            if results.success
                ol
                    each item, index in results.data
                        li
                            a(href="/admision/resultados/"+item.id)= item.name
            else
                p= results.message
