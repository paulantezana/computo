extends ../layout

block content
    include blocks/menu
    
    if results.success
        .SiteHeader
            h1.SiteHeader-title= results.data.name
            .SiteHeader-subTitle Programas de estudio
        .Container.SnMt32.SnMb32
            .Card
                ol
                    each item, index in results.data.content
                        li
                            a(href="/admision/resultados/" + results.data.id + "/"+item.id)= item.name
    else
        p= results.message
